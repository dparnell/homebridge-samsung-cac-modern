    "@types/node-forge": "1.3.11",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "homebridge": "^1.8.2",
    "homebridge-config-ui-x": "^4.63.0",
    "nodemon": "^3.1.1",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
